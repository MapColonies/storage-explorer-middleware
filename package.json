    "@types/jest": "^29.5.14",
    "@types/node": "^14.18.10",
    "@types/supertest": "^2.0.11",
    "@types/unzipper": "^0.10.11",
    "commitlint": "^19.8.1",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^9.23.0",
    "eslint-plugin-jest": "^28.11.0",
